<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<!DOCTYPE sqlMap PUBLIC "-//iBATIS.com//DTD SQL Map 2.0//EN"
"http://www.ibatis.com/dtd/sql-map-2.dtd">
<sqlMap namespace="org.spring.ibatis.example.model.Category">
	<typeAlias alias="category" type="org.spring.ibatis.example.model.Category"/>
	<select id="selectById" resultClass="category" parameterClass="int">
		select * from category where id=#id#
	</select>
</sqlMap>